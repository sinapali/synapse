{
	"name": "store_master_pipeline",
	"properties": {
		"activities": [
			{
				"name": "store_master_copy",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"allowPolyBase": true,
						"polyBaseSettings": {
							"rejectValue": 0,
							"rejectType": "value",
							"useTypeDefault": true
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Sr. No.",
									"type": "Int16"
								},
								"sink": {
									"name": "store_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "Raymark Code",
									"type": "String"
								},
								"sink": {
									"name": "raymark_code",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "Store Name",
									"type": "String"
								},
								"sink": {
									"name": "store_name",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "City",
									"type": "String"
								},
								"sink": {
									"name": "city",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "State",
									"type": "String"
								},
								"sink": {
									"name": "state",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "Region",
									"type": "String"
								},
								"sink": {
									"name": "region",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "MSF Name ",
									"type": "String"
								},
								"sink": {
									"name": "msf_name",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "store_master_excel",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSynapseAnalyticsTable2",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}