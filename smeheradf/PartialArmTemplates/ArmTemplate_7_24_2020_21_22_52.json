						"precision": 10
					},
					{
						"name": "raymark_code",
						"type": "varchar"
					},
					{
						"name": "store_name",
						"type": "varchar"
					},
					{
						"name": "city",
						"type": "varchar"
					},
					{
						"name": "state",
						"type": "varchar"
					},
					{
						"name": "region",
						"type": "varchar"
					},
					{
						"name": "msf_name",
						"type": "varchar"
					}
				],
				"typeProperties": {
					"schema": "stage",
					"table": "store"
				}
			},
			"dependsOn": []
		},
